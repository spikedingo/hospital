<div id="myCarousel" class="carousel slide">
    <div class="carousel-inner">
        <div class="item active">
            <img src="/themes/<%=staticforder%>/images/banner_small.jpg"style="margin:0 auto;">
        </div>
        <div class="item">
            <img src="/themes/<%=staticforder%>/images/banner_small1.jpg"style="margin:0 auto;">
        </div>
    </div>
    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true" style=""></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true" style="color:#5587d6; font-size:30px;"></span>
        <span class="sr-only">Next</span>
    </a>
</div>
