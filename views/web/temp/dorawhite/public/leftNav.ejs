<!--约定cateTypes为分类集合-->
<div class="left-block-wrap nav-list-wrap">
    <h3>分类目录</h3>
    <ul>
    <% for(var i=0;i<currentCateList.length;i++){ 
            if(currentCateList[i].parentID === '0'){%>
    <li class="single-nav">
        <a  data-value="<%=cateTypes[i]._id%>" 
            href="/contentList/<%=currentCateList[i].defaultUrl%>">
            <%=currentCateList[i].name%>
        </a>
    </li>
    <%}}%>
    </ul>
</div>