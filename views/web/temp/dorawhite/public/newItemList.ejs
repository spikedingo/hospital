<!--近期文章模块-->
<div class="left-block-wrap item-list-wrap">
    <h3>近期文章</h3>
    <ul>
        <%if(newItemListData.length>0){%>
            <%newItemListData.forEach(function(item){%>
            <li class="single-content">
                <a 
                target="<%= item.originUrl ? '_blank' : '_self'%>" 
                href="<%= item.originUrl || '/details/' + item._id + '.html' %>"
                >
                    <%=item.stitle%>
                </a>
                <span><i class="fa fa-eye"></i> 555</span>
            </li>
            <%})%>
        <%}else{%>
            <li class="text-center">暂无内容...</li>
        <%}%>
    </ul>
</div>